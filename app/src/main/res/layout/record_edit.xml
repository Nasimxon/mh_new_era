                android:layout_width="2px"
                android:layout_height="match_parent"
                android:background="@color/seriy_calc_line"/>
            <RelativeLayout
                android:id="@+id/rlSubcategory"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:background="@drawable/opeation_frame_greyy"
                android:layout_weight="1">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/ivRecordEditSubCategory"
                        android:layout_width="@dimen/twentyfive_dp"
                        android:layout_height="@dimen/twentyfive_dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginBottom="@dimen/sevent_dp"
                        android:layout_marginTop="@dimen/nine_dp"
                        android:src="@drawable/icons_7"
                        />
                    <TextView
                        android:id="@+id/tvRecordEditSubCategoryName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="eight"
                        android:maxWidth="@dimen/sixty_seven_dp"
                        android:maxLines="2"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:textSize="@dimen/twelve_dp"
                        android:textColor="@color/black_for_myagkiy_glavniy"
                        />
                </LinearLayout>
            </RelativeLayout>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="@color/seriy_calc_line"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">
            <RelativeLayout
                android:id="@+id/rlDot"
                android:background="@drawable/opeation_frame"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_centerInParent="true"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="."
                        android:layout_gravity="center_horizontal"
                        android:textSize="@dimen/thirty_dp"
                        android:textColor="@color/black_for_myagkiy_glavniy"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/decimal"
                        android:textSize="@dimen/fourteen_dp"
                        android:textColor="@color/seriy_calc"
                        />
                </LinearLayout>
            </RelativeLayout>
            <View
                android:layout_width="2px"
                android:layout_height="match_parent"
                android:background="@color/seriy_calc_line"/>
            <RelativeLayout
                android:id="@+id/rlZero"
                android:layout_width="0dp"
                android:background="@drawable/opeation_frame"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:layout_gravity="center_horizontal"
                        android:textSize="@dimen/thirty_dp"
                        android:textColor="@color/black_for_myagkiy_glavniy"/>

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/zero"
                        android:textColor="@color/seriy_calc"
                        android:textSize="@dimen/fourteen_dp" />
                </LinearLayout>
            </RelativeLayout>
            <View
                android:layout_width="2px"
                android:layout_height="match_parent"
                android:background="@color/seriy_calc_line"/>
            <RelativeLayout
                android:id="@+id/rlEqualSign"
                android:layout_width="0dp"
                android:background="@drawable/opeation_frame"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="="
                        android:layout_gravity="center_horizontal"
                        android:textSize="@dimen/thirty_dp"
                        android:textColor="@color/black_for_myagkiy_glavniy"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/equals"
                        android:textSize="@dimen/fourteen_dp"
                        android:textColor="@color/seriy_calc"
                        />
                </LinearLayout>
            </RelativeLayout>
            <View
                android:layout_width="2px"
                android:layout_height="match_parent"
                android:background="@color/seriy_calc_line"/>
            <RelativeLayout
                android:id="@+id/imOKBut"
                android:layout_width="0dp"
                android:background="@drawable/opeation_frame"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="vertical">
                    <ImageView
                        android:layout_width="@dimen/twentyfive_dp"
                        android:layout_height="@dimen/twentyfive_dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginBottom="@dimen/sevent_dp"
                        android:layout_marginTop="@dimen/nine_dp"
                        android:src="@drawable/checkboxx"
                        android:tint="?attr/headAccedent"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/add"
                        android:textSize="@dimen/fourteen_dp"
                        android:gravity="center"
                        android:textColor="?attr/headAccedent"
                        />
                </LinearLayout>
            </RelativeLayout>

        </LinearLayout>
    </LinearLayout>
</LinearLayout>
